<h1>
  <i class="fas fa-upload"></i>
  <%= t('.title') %>
</h1>
<subtitle><%= t('.subtitle') %></subtitle>

<%= simple_form_for @importer do |f| %>
  <div class="upload_button_wrapper">
    <%= f.input :file, label: false %>
  </div>

  <%= f.button :submit, class: 'button-submit' %>
<% end %>

<%= link_to :back, class: 'back_button' do %>
  <i class="fas fa-undo"></i>
  <%= t('.back') %>
<% end %>
